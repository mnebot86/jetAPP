{
	"compilerOptions": {
		"target": "esnext",
		"jsx": "react",
		"module": "commonjs",
		"baseUrl": "./src",
		"paths": {
			"network/*": ["network/*"],
			"navigation/*": ["navigation/*"],
			"screens/*": ["screens/*"]
		},
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"extends": "expo/tsconfig.base"
}
