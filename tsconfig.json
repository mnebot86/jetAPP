{
	"compilerOptions": {
		"target": "esnext",
		"jsx": "react",
		"module": "commonjs",
		"baseUrl": "./src",
		"paths": {
			"network/*": ["network/*"],
			"navigation/*": ["navigation/*"],
			"screens/*": ["screens/*"],
			"images/*": ["images/*"]
		},
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"lib": ["DOM", "DOM.Iterable"]
	},
	"extends": "expo/tsconfig.base"
}
